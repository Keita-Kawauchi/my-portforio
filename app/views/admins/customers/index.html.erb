    <%= render 'layouts/admin.header' %>
<div class="container my-5">
<h2>会員一覧</h2>
<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>氏名</th>
      <th>メールアドレス</th>

      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.full_name %></td>
        <td><%= customer.email %></td>
        <td><%= customer.is_deleted ? "退会":"有効" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
